{
  "name": "hytteadmin",
  "version": "2.0.0-alpha",
  "description": "Cabin management and publishing system for cabins in National Hiking Database",
  "main": "coffee/server.coffee",
  "scripts": {
    "start": "supervisor -w . -e 'node|js|coffee|litcoffee|css|html' -- coffee/server.coffee",
