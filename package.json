{
  "name": "hytteadmin",
  "version": "2.0.0-alpha",
  "description": "Cabin management and publishing system for cabins in National Hiking Database",
  "main": "coffee/server.coffee",
  "scripts": {
    "start": "supervisor -w 'coffee,views' -e 'coffee|html|js' -- coffee/server.coffee",
    "test": "SILENT=true mocha -b -c --check-leaks test/suite.coffee -R progress -r coffee-script -w",
    "test-drone": "node_modules/.bin/mocha test/suite.coffee -R spec -r coffee-script"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Turistforeningen/hytteadmin.git"
  },
  "keywords": [
    "cabin",
    "hiking",
    "trekking",
    "opendata",
    "norway"
  ],
  "author": "Hans Kristian Flaatten",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turistforeningen/hytteadmin/issues"
  },
  "homepage": "https://github.com/Turistforeningen/hytteadmin",
  "dependencies": {
    "supervisor": "~0",
    "mocha": "~1",
    "coffee-script": "~1",
    "hapi": "spumko/hapi",
    "handlebars": "~1",
    "request": "~2"
  }
}
